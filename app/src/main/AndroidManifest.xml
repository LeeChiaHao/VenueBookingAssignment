<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.oop_assignment">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/oop"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/oop"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".UnbookForm" />
        <activity android:name=".Register" />
        <activity android:name=".HistoryMenu" />
        <activity android:name=".BookingForm" />
        <activity android:name=".VenueMenu" />
        <activity android:name=".MainMenu" />
        <activity android:name=".LogIn">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>