<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.DayNight">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <!-- Event type, Start time, end time spinner 's option-->
    <string-array name="eventType">
        <item>Club Activity</item>
        <item>Student Discussion</item>
        <item>Training</item>
    </string-array>

    <string-array name="timeOption">
        <item>7 pm</item>
        <item>8 pm</item>
        <item>9 pm</item>
        <item>10 pm</item>
        <item>11 pm</item>
        <item>12 pm</item>
    </string-array>
</resources>
